
@{
    ViewBag.Title = "DangKy";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Đăng ký</h2>
<h2 class="text-danger">@TempData["err"]</h2>
<form action="/Auth/Signin" method="post">
    User Name: <input type="text" class="form-control" name="taikhoan" placeholder="username" />
    <br />
    Password: <input type="password" class="form-control"  name="matkhau" placeholder="password" />
    <br />
    Re-Password: <input type="password" class="form-control"  name="rematkhau" placeholder="re-password" />
    <input type="submit" class="form-control"  value="Signin" />
</form>

